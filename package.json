    "@types/warning": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "check-engines": "^1.5.0",
    "connected-react-router": "^6.7.0",
    "coveralls": "*",
